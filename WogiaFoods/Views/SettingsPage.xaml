<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WogiaFoods.Views.SettingsPage" Visual="Material">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Manage Categories" x:Name="BtnCategories" Clicked="BtnCategories_Clicked"/>
            <Button Text="Manage Products" x:Name="BtnProducts" Clicked="BtnProducts_Clicked"/>
            <Button Text="Manage Cart" x:Name="BtnCart" Clicked="BtnCart_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>